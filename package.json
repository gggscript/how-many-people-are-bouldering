{
  "name": "how-many-people-are-bouldering",
  "version": "1.0.0",
  "description": "An API Client to check how many people are in a bouldering gym (in Germany)",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "author": "gggscript",
  "license": "CC0-1.0",
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/gggscript/how-many-people-are-bouldering",
  "bugs": "https://github.com/gggscript/how-many-people-are-bouldering/issues",
  "repository": {
    "type": "git",
    "url": "git@github.com:gggscript/how-many-people-are-bouldering.git"
  },
  "scripts": {
    "build": "rimraf dist && tsc",
    "dev": "rimraf dist && tsc --watch",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "prepare": "husky install"
  },
  "keywords": [
    "rock climbing",
    "bouldering",
    "climbing",
    "occupancy",
    "headcount",
    "gym",
    "germany"
  ],
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.11.10",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "husky": "^8.0.2",
    "prettier": "^2.8.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "got-scraping": "^3.2.12",
    "jsdom": "^20.0.3"
  }
}
